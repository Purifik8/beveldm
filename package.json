{
  "name": "beveldm",
  "productName": "Bevel Download Manager",
  "author": "Tari Ibaba <tariibabadev@gmail.com>",
  "version": "0.1.0",
  "private": true,
  "main": "public/electron.js",
  "homepage": ".",
  "build": {
    "appId": "com.tariibaba.beveldm",
    "productName": "Bevel Download Manager",
    "icon": "images/app-icon.png",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "win": {
      "target": [
        {
          "target": "msi",
          "arch": ["ia32", "x64"]
        },
        {
          "target": "zip",
          "arch": ["ia32", "x64"]
        }
      ]
    },
    "mac": {
      "target": ["zip"]
    },
    "linux": {
      "target": ["deb", "rpm", "tar.gz"]
    }
  },
  "dependencies": {
    "@material-ui/core": "^4.5.1",
    "@material-ui/icons": "^4.5.1",
    "await-timeout": "^1.0.1",
    "clsx": "^1.0.4",
    "compare-urls": "^2.0.0",
    "content-disposition-filename": "^0.0.1",
    "electron-devtools-installer": "^2.2.4",
    "electron-is-dev": "^1.1.0",
    "electron-store": "^5.0.0",
    "filename-reserved-regex": "^2.0.0",
    "hex-to-rgba": "^2.0.1",
    "is-online": "^8.2.1",
    "lodash.groupby": "^4.6.0",
    "moment": "^2.24.0",
    "open": "^7.0.1",
    "path-exists": "^4.0.0",
    "pify": "^4.0.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-redux": "^7.1.0",
    "react-scripts": "3.0.1",
    "redux": "^4.0.4",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0",
    "remote-file-info": "^1.0.1",
    "sum-by": "^1.0.3",
    "typeface-roboto": "^0.0.75",
    "uuid": "^3.3.2",
    "valid-url": "^1.0.9",
    "when-expression": "^0.0.2",
    "youtube-url": "^0.5.0",
    "ytdl-core": "^2.0.0"
  },
  "scripts": {
    "start": "rescripts start",
    "build": "rescripts build",
    "test": "rescripts test",
    "eject": "react-scripts eject",
    "preelectron-pack": "yarn build",
    "electron-pack": "electron-builder --dir",
    "preelectron-dist": "yarn build",
    "electron-dist": "electron-builder"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@rescripts/cli": "^0.0.11",
    "@rescripts/rescript-env": "^0.0.10",
    "electron": "^8.1.1",
    "electron-builder": "^22.4.1",
    "file-loader": "^5.0.2"
  }
}
